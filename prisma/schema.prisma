generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bac {
  an                     Int
  cod_candidat           String
  id_judet               String?
  id_liceu               String?
  sex                    String?
  promotie_anterioara    String
  specializare           String
  lr_init                Float?  @db.Real
  lr_cont                Float?  @db.Real
  lr_final               Float?  @db.Real
  limba_materna          String?
  lm_init                Float?  @db.Real
  lm_cont                Float?  @db.Real
  lm_final               Float?  @db.Real
  limba_moderna          String
  disciplina_obligatorie String
  do_init                Float?  @db.Real
  do_cont                Float?  @db.Real
  do_final               Float?  @db.Real
  disciplina_alegere     String
  da_init                Float?  @db.Real
  da_cont                Float?  @db.Real
  da_final               Float?  @db.Real
  medie                  Float?  @db.Real
  my_medie               Float?  @db.Real
  rezultat               String

  @@id([an, cod_candidat], map: "idx_16399_sqlite_autoindex_bac_1")
}

model en {
  an                      Int
  cod_candidat            String
  id_judet                String
  id_scoala               String
  lr_final                Float?  @db.Real
  ma_final                Float?  @db.Real
  limba_materna           String?
  lm_final                Float?  @db.Real
  medie_en                Float?  @db.Real
  medie_abs               Float?  @db.Real
  medie_adm               Float?  @db.Real
  repartizat_id_liceu     String?
  repartizat_specializare String?

  @@id([an, cod_candidat], map: "idx_16384_sqlite_autoindex_en_1")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model alias {
  alternativ String @id(map: "idx_16394_sqlite_autoindex_alias_1")
  corect     String
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model institutii {
  id              String  @id(map: "idx_16404_sqlite_autoindex_institutii_1")
  nume            String
  rank            Int?
  adresa          String?
  website         String?
  email           String?
  telefon         String?
  wikipedia       String?
  facebook        String?
  instagram       String?
  youtube         String?
  cod_siiir       String  @unique(map: "idx_16404_sqlite_autoindex_institutii_2")
  cod_fiscal      String?
  liceu_legacy    String?
  gimnaziu_legacy String?
  latlong         String?
  sigla           String?
  sigla_lg        String?
  sigla_xs        String?
  last_updated    BigInt?
  last_author     String?
  sigla_lipsa     Boolean @default(false)
}

model siiir {
  cod                   String  @id(map: "idx_16389_sqlite_autoindex_siiir_1")
  aux_id                String? @unique(map: "idx_16389_index_aux_id")
  denumire              String?
  denumire_scurta       String?
  localitate            String?
  localitate_superioara String?
  judet                 String?
  statut                String?
  tip_unitate           String?
  forma_proprietate     String?
  strada                String?
  nr_strada             String?
  cod_postal            String?
  website               String?
  telefon               String?
  cod_sirues            String?
  cod_fiscal            String?
  mod_operare           String?
}

model users {
  id       String @id
  email    String
  password String
  name     String
}

model bac_new {
  an                     Int
  cod_candidat           String
  sex                    String?
  specializare           String
  promotie_anterioara    Boolean
  unitate_siiir          String?
  unitate_nume           String?
  unitate_cod_judet      String?
  clasa                  String?
  lr_init                Decimal? @db.Decimal
  lr_cont                Decimal? @db.Decimal
  lr_final               Decimal? @db.Decimal
  limba_materna          String?
  lm_init                Decimal? @db.Decimal
  lm_cont                Decimal? @db.Decimal
  lm_final               Decimal? @db.Decimal
  disciplina_obligatorie String
  do_init                Decimal? @db.Decimal
  do_cont                Decimal? @db.Decimal
  do_final               Decimal? @db.Decimal
  disciplina_alegere     String
  da_init                Decimal? @db.Decimal
  da_cont                Decimal? @db.Decimal
  da_final               Decimal? @db.Decimal
  limba_moderna          String
  medie                  Decimal? @db.Decimal
  rezultat               String

  @@id([an, cod_candidat], map: "bac_pkey")
}

